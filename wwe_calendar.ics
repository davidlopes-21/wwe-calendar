        opponent = card.find(</span><span><span class="hljs-string">"div"</span></span><span>, class_=</span><span><span class="hljs-string">"match-versus"</span></span><span>).text.strip()  </span><span><span class="hljs-comment"># vs outro clube</span></span><span>

        </span><span><span class="hljs-comment"># Separar e converter data</span></span><span>
        date_parts = date_raw.split(</span><span><span class="hljs-string">" - "</span></span><span>)
        </span><span><span class="hljs-keyword">if</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(date_parts) != </span><span><span class="hljs-number">2</span></span><span>:
            </span><span><span class="hljs-keyword">continue</span></span><span>

        date_str = </span><span><span class="hljs-string">f"2025/<span class="hljs-subst">{date_parts[<span class="hljs-number">0</span></span></span></span><span>]} </span><span><span class="hljs-subst">{date_parts[<span class="hljs-number">1</span></span></span><span>]}"
        game_cdmx = tz_cdmx.localize(datetime.strptime(date_str, </span><span><span class="hljs-string">"%Y/%m/%d %I:%M %p"</span></span><span>))
        game_lisboa = game_cdmx.astimezone(tz_lisboa)

        </span><span><span class="hljs-comment"># Criar evento</span></span><span>
        event = Event()
        event.name = </span><span><span class="hljs-string">f"Los Aliens FC vs <span class="hljs-subst">{opponent}</span></span></span><span>"
        event.begin = game_lisboa
        event.duration = timedelta(hours=</span><span><span class="hljs-number">1</span></span><span>)
        event.description = </span><span><span class="hljs-string">"Jogo da Kings League Am√©ricas"</span></span><span>

        cal.events.add(event)

