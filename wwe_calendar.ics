        event.begin = game_datetime_lisboa
        event.duration = timedelta(hours=</span><span><span class="hljs-number">1</span></span><span>)
        event.description = </span><span><span class="hljs-string">"Jogo da Kings League Am√©ricas"</span></span><span>

        cal.events.add(event)

