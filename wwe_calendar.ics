        </span><span><span class="hljs-keyword">if</span></span><span> period == </span><span><span class="hljs-string">"AM"</span></span><span> </span><span><span class="hljs-keyword">and</span></span><span> hour == </span><span><span class="hljs-number">12</span></span><span>:
            hour = </span><span><span class="hljs-number">0</span></span><span>

        game_datetime_cdmx = datetime(</span><span><span class="hljs-number">2025</span></span><span>, </span><span><span class="hljs-built_in">int</span></span><span>(month), </span><span><span class="hljs-built_in">int</span></span><span>(day), hour, minute)
        game_datetime_cdmx = tz_cdmx.localize(game_datetime_cdmx)
